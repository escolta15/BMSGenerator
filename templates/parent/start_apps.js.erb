const { exec } = require('child_process');

const apps = [];

if (apps.length) {
  exec(`concurrently ${apps.join(' ')}`, (err, stdout, stderr) => {
    if (err) {
        console.error(`Error starting the apps comand: ${err}`)
        return;
    }
    console.log(stdout);
    if (stderr) {
        console.error(`stderr: ${stderr}`);
    }
  });
} else {
  console.error('There are not any apps yet');
}